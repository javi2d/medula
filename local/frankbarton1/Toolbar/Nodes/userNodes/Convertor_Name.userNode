


Class = 'ModifyMetaData'


class Convertor_Name:
	
	conv_name = strKnob( 'output name' , '' )



def onCreate( this ):
	
	this.NODE.setName( 'Output_Name' )
	
	this.NODE.readKnobs( '\nmetadata {\n {set convertor/output_name "\\[value conv_name]"}\n}' )
	
	this.KNOBS.label.setValue( '[value conv_name]' )


