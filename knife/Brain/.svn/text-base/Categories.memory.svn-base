
'''
TAGS

'$CATEGORY' : this.VALUES.wm_category, 
'$LABEL'    : this.VALUES.wm_label , 
'$UNITID'   : this.UNIT_ID ,
'$UNITNAME' : this.UNIT_NAME, 





'''


class Export:

	params  = 'channels rgb  colorspace default file_type jpeg'
	color = 'Red'

class Production:

	pattern  = 'vfx_$UNITID_$LABEL'
	params  = 'channels rgb  colorspace default file_type dpx'
	

class Daily:
	
	pattern = None
	params = 'channels rgb colorspace default file_type exr'


class Cache:

	params = 'channels all colorspace default file_type exr'
	

# Snapshot renders only current frame

class Snapshot:

	pattern  = 'snapshot_$LABEL'
	params = 'channels all colorspace default file_type exr'
	color = 'White'